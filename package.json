{
  "name": "templatefrontend",
  "version": "1.0.0",
  "description": "Template for React and SCSS with Webpack",
  "main": "index.js",
  "scripts": {
    "build:vendor": "webpack --config ./webpack.vendor.config.js --mode production",
    "build:project": "webpack --config ./webpack.config.js --mode production",
    "build": "npm run build:vendor && npm run build:project",
    "start": "webpack serve --config ./webpack.dev.config.js --mode development",
    "deploy": "vercel --prod",
    "install:vercel": "npm i -g vercel",
    "analyzer": "npx webpack --profile --json > stats.json && npx webpack-bundle-analyzer stats.json ./dist"
  },
