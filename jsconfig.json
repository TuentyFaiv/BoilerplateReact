{
  "compilerOptions": {
    "target": "esnext",
    "allowSyntheticDefaultImports": false,
    "baseUrl": "./",
    "paths": {
      "@images/*": [
        "src/assets/images/*"
      ],
      "@icons/*": [
        "src/assets/images/icons/*"
      ],
      "@videos/*": [
        "src/assets/videos/*"
      ],
      "@fonts/*": [
        "src/assets/fonts/*"
      ],
      "@boot": [
        "src/common/boot.js"
      ],
      "@functions": [
        "src/common/functions.js"
      ],
      "@routes": [
        "src/common/routes.js"
      ],
      "@schemas": [
        "src/common/schemas/index.js"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@containers/*": [
        "src/containers/*"
      ],
      "@context": [
        "src/context/AppContext.jsx"
      ],
      "@hooks": [
        "src/hooks/index.js"
      ],
      "@hoc": [
        "src/hoc/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@stylesComponents/*": [
        "src/styles/components/*"
      ],
      "@stylesPages/*": [
        "src/styles/pages/*"
      ],
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}